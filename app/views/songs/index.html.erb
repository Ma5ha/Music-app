





  <table class="table">
  <thead class="thead-light">
    <tr>
      <th scope="col">id</th>
      <th scope="col">Artrist</th>
      <th scope="col">Song</th>
      <th scope="col">Album</th>
      
    </tr>
  </thead>
 
  <tbody>
  
    <% @songs.each do |song| %>
   <tr>
      <th scope="row"><%=song.id%></th>
      <td><%=song.artrist%></td>
      <div>
      <td><%=song.name  %> <%=link_to song.link, "rel" =>"noopener noreferrer", "target"=>"_blank">"open video"  do %>
      <% octicon "play" %>
      <%end%>  
      </td>
      </div >
      <td><%=song.album%></td>
    </tr>
    <% end %>  

</table>





